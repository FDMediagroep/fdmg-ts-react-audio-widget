{
  "name": "@fdmg/ts-react-audio-widget",
  "version": "1.0.21",
  "description": "ReactJS AudioWidget component",
  "main": "dist/AudioWidget.js",
  "types": "dist/AudioWidget.d.ts",
  "scripts": {
    "lint": "tslint -p .",
    "release": "npm update && npm install && yarn && npm run test && npm run lint && npm run sass && npm run tsc",
    "sass": "node-sass --importer=node_modules/node-sass-tilde-importer --output-style=compressed scss/AudioWidget.scss > dist/AudioWidget.css",
    "tsc": "tsc -d",
    "test": "jest",
    "updateSnapshot": "jest --updateSnapshot",
    "u": "npm test -- -u",
    "webpack-demo": "webpack --mode production"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:FDMediagroep/fdmg-ts-react-audio-widget.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "author": "Willem Liu, Alexander Eerenberg",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.3",
    "@types/react": "^16.4.14",
    "awesome-typescript-loader": "^5.2.1",
    "coveralls": "^3.0.2",
    "css-loader": "^0.28.10",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.4.0",
    "enzyme-to-json": "^3.3.4",
    "extract-text-webpack-plugin": "^4.0.0-alpha.0",
    "file-loader": "^1.1.9",
    "jest": "^22.4.4",
    "jest-cli": "^23.0.0",
    "node-sass": "^4.9.3",
    "node-sass-tilde-importer": "^1.0.1",
    "preact": "^8.3.1",
    "preact-compat": "^3.18.4",
    "raf": "^3.4.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-test-renderer": "^16.5.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "ts-jest": "23.1.4",
    "tslint": "^5.11.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.2",
    "webpack": "^4.20.2",
    "webpack-cli": "^3.1.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "setupFiles": [
      "raf/polyfill"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ],
    "collectCoverage": true,
    "coverageDirectory": "dist/coverage",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "text-summary"
    ]
  },
  "dependencies": {
    "@fdmg/ts-react-progress-bar": "^1.0.5"
  }
}
